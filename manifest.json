{
    "manifest_version": 2,
    "name": "AdBlock Notice Removal",
    "version": "1.0.0",
    "author": "Jason M",
    "description": "Remove the adblock notice on popular sites",
    "minimum_chrome_version": "52",
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["arrive.min.js", "contentscript.js"],
        "run_at": "document_start"
      }
    ],
    "web_accessible_resources": [
      "rules.json"
    ]
  }